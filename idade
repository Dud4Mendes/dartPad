void main() {
  String nome = 'duda';
  int idade = 160;
  
  if (idade <= 13 && nome == 'duda'){
    print('${nome} é criança');
  } else if (idade >13 && idade <18){
    print('${nome} é adolecente');
  } else if (idade >18 && idade <59){
    print('${nome} é adulto');
  } else if (idade >=60 && idade <200){ 
    print('${nome} é idoso');
  }else{
   print('dado invalido');
  }
}
